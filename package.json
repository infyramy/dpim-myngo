{
  "name": "myNGO",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A modern Vue 3 boilerplate with TypeScript, Shadcn UI, and more",
  "keywords": [
    "vue",
    "vue3",
    "boilerplate",
    "typescript",
    "shadcn",
    "starter",
    "template"
  ],
  "license": "MIT",
  "author": {
    "name": "Your Name",
    "email": "your-email@example.com"
  },
  "bin": {
    "jboilerplate": "./src/cli/index.js"
  },
  "files": [
    "dist",
    "templates",
    "src/cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/jboilerplate"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch src/server/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vue-tsc --noEmit && vite build",
    "build:backend": "tsc --project tsconfig.server.json",
    "build:production": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "preview:production": "vite preview --port 3000 --host 0.0.0.0",
    "start": "node dist/server/index.js",
    "start:frontend": "serve -s dist -l 3000",
    "start:backend": "node dist/server/index.js",
    "start:production": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "docker:build": "docker build -t myngo-app .",
    "docker:build:frontend": "docker build -f Dockerfile.frontend -t myngo-frontend .",
    "docker:build:backend": "docker build -f Dockerfile.backend -t myngo-backend .",
    "docker:run": "docker run -p 3000:3000 myngo-app",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "prepare": "pnpm run build",
    "setup": "pnpm install && pnpm run build"
  },
